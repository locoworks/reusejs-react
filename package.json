{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "bundle": "turbo run bundle",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run bundle && changeset publish",
    "button:dev": "turbo run dev --filter=button-app...",
    "button:build": "turbo run build --filter=@locoworks/reusejs-react-button...",
    "docs:dev": "yarn list:comp && turbo run dev --filter=reuse-docs... --concurrency 12",
    "autogen:comp": "node ./scripts/scaffoldComponent.mjs",
    "gen:example": "node ./scripts/createExample.mjs",
    "input-group:dev": "turbo run dev --filter=input-group-app...",
    "input-group:build": "turbo run build --filter=@locoworks/reusejs-react-input-group...",
    "progress-bar:dev": "turbo run dev --filter=progress-bar-app...",
    "progress-bar:build": "turbo run build --filter=@locoworks/reusejs-react-progress-bar...",
    "input:dev": "turbo run dev --filter=input-app...",
    "select-dropdown:dev": "turbo run dev --filter=select-dropdown-app...",
    "select-dropdown:build": "turbo run build --filter=@locoworks/reusejs-react-select-dropdown...",
    "otp-input:dev": "turbo run dev --filter=otp-input-app...",
    "otp-input:build": "turbo run build --filter=@locoworks/reusejs-react-otp-input...",
    "input:build": "turbo run build --filter=@locoworks/reusejs-react-input...",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "list:comp": "node ./scripts/getAllComponentsList.mjs",
    "build:docs": "node ./scripts/buildDocs.mjs && yarn list:comp && turbo run build --filter=reuse-docs...",
    "toolkit:dev": "turbo run dev --filter=toolkits-app...",
    "clean": "node ./scripts/cleanBuilds.mjs",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@betalectic-reusejs/shared-config-eslint-reactts": "^1.0.1",
    "@betalectic-reusejs/shared-config-prettier-ts": "^1.0.0",
    "@changesets/cli": "^2.26.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.45.0",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-standard": "^5.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "path": "^0.12.7",
    "postcss": "^8.4.23",
    "prettier": "^3.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.3.2"
  },
  "name": "reusev2-sample",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "components/*",
    "development/*",
    "docs",
    "packages/*",
    "tsconfig",
    "toolkit/*"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.22.7",
    "@docsearch/react": "^3.5.1",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.2",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/istanbul-lib-coverage": "^2.0.4",
    "@types/mdx": "^2.0.5",
    "@types/node": "^20.2.0",
    "babel-loader": "^9.1.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "framer-motion": "^10.12.17",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-extract-toc": "^1.1.0",
    "remark-parse": "^10.0.2",
    "rollup": "^3.21.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tailwind-merge": "^1.12.0",
    "turbo": "^1.9.8",
    "typescript": "^5.0.4",
    "unified": "^10.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
